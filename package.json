{
  "name": "KADE",
  "description": "Analysis Tool for Q Methodology",
  "version": "1.2.0",
  "private": true,
  "homepage": "./",
  "keywords": [],
  "license": "LGPL-3.0",
  "author": {
    "name": "Shawn Banasick",
    "email": "ken.q.tools@gmail.com",
    "url": "https://github.com/shawnbanasick/kade_v2"
  },
  "build": {
    "afterSign": "scripts/notarize.js",
    "appId": "com.banasick.KADE",
    "productName": "KADE",
    "copyright": "Copyright Â© 2020 Shawn Banasick",
    "linux": {
      "category": "Science",
      "description": "Open source Q methodology app",
      "synopsis": "Q methodology app",
      "icon": "../build/icons/icon.icns",
      "target": [
        {
          "target": "deb",
          "arch": [
            "ia32",
            "x64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "ia32",
            "x64"
          ]
        },
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "snap",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "../build/icons/icon3.png",
      "target": [
        "portable",
        "nsis"
      ]
    },
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "target": [
        "dmg"
      ]
    },
    "files": [
      "build/icon.*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "./build"
    }
  },
  "dependencies": {
    "@ag-grid-community/all-modules": "^22.0.0",
    "@ag-grid-community/react": "^22.0.0",
    "@trodi/electron-splashscreen": "^0.3.4",
    "a11y-react-tabs": "^0.1.2",
    "ag-grid-react": "^22.0.0",
    "cross-env": "^6.0.3",
    "d3": "^5.14.2",
    "dotenv": "^8.2.0",
    "electron-fetch": "^1.4.0",
    "electron-is-dev": "^1.1.0",
    "electron-settings": "^3.2.0",
    "electron-splashscreen": "^1.0.0",
    "file-saver": "^2.0.2",
    "i18next": "^19.0.0",
    "i18next-chained-backend": "^2.0.0",
    "i18next-localstorage-backend": "^3.0.0",
    "i18next-node-fs-backend": "^2.1.3",
    "lodash": "^4.17.15",
    "papaparse": "^5.1.0",
    "react": "^16.12.0",
    "react-color": "^2.17.3",
    "react-dom": "^16.12.0",
    "react-easy-state": "^6.1.3",
    "react-i18next": "^11.2.1",
    "react-icons": "^3.9.0",
    "react-is": "^16.12.0",
    "react-scripts": "3.2.0",
    "react-split-pane": "^0.1.89",
    "react-toastify": "^5.4.1",
    "react-toggle": "^4.1.1",
    "react-window-size-listener": "^1.5.4",
    "rfdc": "^1.1.4",
    "save-svg-as-png": "^1.4.14",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "semver": "^7.1.3",
    "styled-components": "^5.0.0",
    "xlsx": "^0.15.3"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron . --no-sandbox\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "build -l",
    "dist:macOS": "electron-builder --mac",
    "dist:windows": "electron-builder --windows --x64 --ia32",
    "dist:linux": "electron-builder --linux"
  },
  "main": "public/electron.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.13",
    "@rescripts/rescript-env": "^0.0.11",
    "babel-eslint": "10.0.3",
    "concurrently": "^5.0.0",
    "electron": "^7.1.1",
    "electron-builder": "^22.1.0",
    "electron-notarize": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "1.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2",
    "wait-on": "^3.3.0"
  }
}
